pipeline {
    agent any 
    stages {
        stage('Test stage') {
            steps {
                echo "${JOB_NAME}"
            }
        }
        stage('Return downstream info') {
            steps {
                echo "${myjob.getResult()}"
            }
        }
    }
    post {
        always { 
            echo "Jenkins URL is - ${JENKINS_URL}"
        }
    }
}
